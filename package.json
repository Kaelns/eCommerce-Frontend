{
  "name": "e-commerce",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "node ./dist/index.js",
    "build": "tsc && vite build",
    "start:preview": "vite preview",
    "start:dev": "vite",
    "test": "vitest",
    "test:staged": "vitest related --run",
    "test:changed:last:commit": "vitest run --changed HEAD~1",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .tsx,.ts",
    "lint:fix": "eslint --fix --ext .ts,.tsx .",
    "format": "prettier --config .prettierrc src/**/*.tsx --check",
    "format:fix": "prettier --config .prettierrc src/**/*.tsx --write",
    "prepare": "husky"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.4.0",
    "@mui/material": "^6.4.0",
    "@mui/system": "^6.4.0",
    "@mui/x-date-pickers": "^7.23.6",
    "@reduxjs/toolkit": "^2.5.0",
    "dayjs": "^1.11.13",
    "lodash-es": "^4.17.21",
    "qs": "^6.14.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.1.1",
    "react-slick": "^0.30.3",
    "sass": "^1.83.4",
    "slick-carousel": "^1.8.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@commercetools/platform-sdk": "^8.1.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/lodash": "^4.17.14",
    "@types/node": "^22.10.6",
    "@types/qs": "^6.9.18",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "@types/react-slick": "^0.23.13",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-perfectionist": "^4.7.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "eslint-plugin-vitest": "^0.5.4",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.3.0",
    "prettier": "^3.4.2",
    "typescript": "^5.7.3",
    "vite": "^6.0.7",
    "vite-plugin-svgr": "^4.3.0",
    "vitest": "^2.1.8"
  }
}
